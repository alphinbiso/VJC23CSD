<!DOCTYPE html>
<html>
<head>
<title>Fibonacci Series</title>
</head>
<body>
<script>
    const l = parseInt(prompt("Enter limit: "));
    let a = 0;
    let b = 1;
    document.write('<h3>Fibonacci Series:</h3>');
    document.write(a + " ");
    document.write(b + " ");
    let c = 0;
    for (let i = 1; i <= l - 1; i++) {
        c = a + b;
        document.write(c + " ");
        a = b;
        b = c;
    }
</script>
</body>
</html>
