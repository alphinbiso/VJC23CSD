<!DOCTYPE html>
<html>
<head>
<title>PRIME NUMBER</title>
</head>
<body>
<script>
const x=parseInt(prompt("Enter starting number of Interval"));
const y=parseInt(prompt("Enter ending number of Interval"));
document.write(<h3>Prime numbers between ${x} and ${y}:</h3>);
for(let i = x; i <= y; i++){
    let count = 0;
    for(let j = 1; j <= i; j++){
        if(i % j === 0){
            count++;
        }
    }
    if(count === 2){
        document.write(i + "<br>");
    }
}
</script>
</body>
</html>
